<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".LobbyScreen">

    <TextView
        android:id="@+id/textViewLobbyName"
        android:layout_width="126dp"
        android:layout_height="29dp"
        android:layout_marginStart="150dp"
        android:layout_marginTop="19dp"
        android:layout_marginEnd="150dp"
        android:text="{Lobby Name}"
        app:autoSizeTextType="uniform"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.533"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="134dp"
        android:layout_height="38dp"
        android:layout_marginStart="116dp"
        android:layout_marginTop="14dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="134dp"
        android:layout_height="38dp"
        android:layout_marginStart="116dp"
        android:layout_marginTop="11dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewLobbyName" />

    <Button
        android:id="@+id/button2"
        android:layout_width="61dp"
        android:layout_height="37dp"
        android:layout_marginTop="11dp"
        android:layout_marginEnd="89dp"
        android:background="#EB052D"
        android:backgroundTint="@color/black"
        android:text="X"
        android:textColor="@color/white"
        android:textColorLink="#C81313"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewLobbyName" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="134dp"
        android:layout_height="38dp"
        android:layout_marginStart="116dp"
        android:layout_marginTop="18dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <Button
        android:id="@+id/buttonKickDetective1"
        android:layout_width="103dp"
        android:layout_height="40dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="15dp"
        android:text="X"
        app:layout_constraintStart_toEndOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/button2" />

    <Button
        android:id="@+id/buttonKickDetective2"
        android:layout_width="103dp"
        android:layout_height="40dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="16dp"
        android:text="X"
        app:layout_constraintStart_toEndOf="@+id/textView10"
        app:layout_constraintTop_toBottomOf="@+id/buttonKickDetective1" />

    <Button
        android:id="@+id/buttonKickDetective3"
        android:layout_width="103dp"
        android:layout_height="40dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="14dp"
        android:text="X"
        app:layout_constraintStart_toEndOf="@+id/textView8"
        app:layout_constraintTop_toBottomOf="@+id/buttonKickDetective2" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="134dp"
        android:layout_height="38dp"
        android:layout_marginStart="116dp"
        android:layout_marginTop="16dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView10" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="134dp"
        android:layout_height="38dp"
        android:layout_marginStart="116dp"
        android:layout_marginTop="14dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <Button
        android:id="@+id/buttonKickDetective4"
        android:layout_width="103dp"
        android:layout_height="40dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="12dp"
        android:text="X"
        app:layout_constraintStart_toEndOf="@+id/textView9"
        app:layout_constraintTop_toBottomOf="@+id/buttonKickDetective3" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="134dp"
        android:layout_height="38dp"
        android:layout_marginStart="116dp"
        android:layout_marginTop="16dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />

    <Button
        android:id="@+id/buttonKickDetective5"
        android:layout_width="103dp"
        android:layout_height="40dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="15dp"
        android:text="X"
        app:layout_constraintStart_toEndOf="@+id/textView11"
        app:layout_constraintTop_toBottomOf="@+id/buttonKickDetective4" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="236dp"
        android:layout_height="257dp"
        android:layout_marginStart="87dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="88dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo" />

    <Button
        android:id="@+id/buttonStartGame"
        android:layout_width="103dp"
        android:layout_height="40dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="98dp"
        android:layout_marginBottom="26dp"
        android:text="Start"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonKickDetective5"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/buttonCancelGame"
        android:layout_width="103dp"
        android:layout_height="40dp"
        android:layout_marginStart="75dp"
        android:layout_marginTop="26dp"
        android:layout_marginBottom="26dp"
        android:text="Cancel"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView11"
        app:layout_constraintVertical_bias="0.0" />

</androidx.constraintlayout.widget.ConstraintLayout>